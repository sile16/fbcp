    "version": "0.2.0",
    "configurations": [
        {
            "name": "Mount Show",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-showmounts"]
        },
        {
            "name": "Local Spread Copy",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-threads", "16","-verify" ,"-progress", "-verbose", "../junk1", "../junk2"]
        },
        {
            "name": "Local Spread Copy copyv2",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-threads", "5","-copyv2","-sizeMB", "32", "-verify", "-verbose", "../junk1", "../junk2", ]
        },
        {
            "name": "Local to Remote Spread Copy",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-threads", "5",  "-verbose", "../junk1", "127.0.0.1:/Volumes/RAMDisk/junk2",]
        },
        {
            "name": "Remote to Local Spread Copy",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-threads", "5", "-verbose", "-verify", "127.0.0.1:/Volumes/RAMDisk/junk1", "/dev/null", ]
        },
        {
            "name": "Benchmark Test",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [ "-verbose", "-threads", "5", "-sizeMB", "16", "-benchmark", "-verify","127.0.0.1:/Volumes/RAMDisk/junk2"]
        },
        {
            "name": "Stream Test - StreamIN",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [ "-threads", "5", "-force-input-stream", "-verbose", "-verify", "../junk1", "../junk2"]
        }
        ,
        {
            "name": "Stream Test - StreamOut",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [ "-threads", "5", "-force-output-stream", "-verbose", "-verify", "../junk1", "../junk2"]
        },
        {
            "name": "Stream Test stream plaid ",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [ "-threads", "5", "-stream", "-verbose","-verify", "../junk1", "../junk8"]
        },
        {
            "name": "Stream Test Local Pliad ",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [ "-threads", "5", "-stream", "-verbose", "../junk1", "../junk9"]
        },
        {
            "name": "Hash spread Test Local  ",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-hash", "-threads", "16", "-verbose", "../junk2",]
        },
        {
            "name": "Hash spread Test Local  64 G",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-hash", "-threads", "1", "-verbose", "tempdir/67108864",]
        },
        {
            "name": "Hash spread Test Local (actually nfscopy) 64 G",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-verify", "-threads", "1", "tempdir/67108864","/dev/null"]
        },
        {
            "name": "Hash spread Test remote  ",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["-hash", "-threads", "5", "-verbose", "127.0.0.1:/Volumes/RAMDisk/junk2"]
        }
        
    ]
}